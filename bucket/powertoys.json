        "script": [
            "Expand-DarkArchive \"$dir\\$fname\" \"$dir\\.tmp\"",
            "Get-ChildItem \"$dir\\.tmp\\AttachedContainer\\PowerToysUserSetup*.msi\" | Rename-Item -NewName 'PowerToysSetup.msi' -Force",
