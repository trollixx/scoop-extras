            "ArmCord"
        ]
    ],
    "checkver": "github",
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/ArmCord/ArmCord/releases/download/v$version/ArmCord.Setup.$version.exe#/dl.7z"
            }
        },
        "hash": {
            "url": "$baseurl/latest.yml",
            "regex": "sha512:\\s$base64"
        }
    }
}
