{
    "version": "1.1.0",
    "description": "A custom client designed to enhance your Discord experience while keeping everything lightweight.",
    "homepage": "https://github.com/ArmCord/ArmCord",
    "license": "OSL-3.0",
    "architecture": {
        "64bit": {
            "url": "https://github.com/ArmCord/ArmCord/releases/download/v1.1.0/ArmCord.Setup.1.1.0.exe#/dl.7z",
            "hash": "sha512:9cd52cc3709548d03d4f13c722826526c1b500bcd632918707c9dd57870d9f952d57a4a0d47f9447b64618e27113570ea26a020b631d510fff7571a984c3633f",
            "pre_install": [
                "Expand-7zipArchive \"$dir\\`$PLUGINSDIR\\app-64.7z\" \"$dir\"",
