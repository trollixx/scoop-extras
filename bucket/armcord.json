            "hash": "sha512:801edc0c32721a77443b242e453ec16b72fd6b8a91be96b921c4d99bac319c971ad87dbb0b3215a340b88737aa1d7e02dd86536ed780b6b9d8f3dab6877527e5",
            "pre_install": [
