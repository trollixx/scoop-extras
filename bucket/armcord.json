{
    "version": "1.1.1",
    "description": "A custom client designed to enhance your Discord experience while keeping everything lightweight.",
    "homepage": "https://github.com/ArmCord/ArmCord",
    "license": "OSL-3.0",
    "architecture": {
        "64bit": {
            "url": "https://github.com/ArmCord/ArmCord/releases/download/v1.1.1/ArmCord.Setup.1.1.1.exe#/dl.7z",
            "hash": "sha512:033d496ca750060726eee048fb7799c4dab029e86e6bb7dfe13fd9a252266e3cef12f059ec12adc5ee71be1ac46f67967b9838abc4ea0fbfed0514d81e458350",
            "pre_install": [
                "Expand-7zipArchive \"$dir\\`$PLUGINSDIR\\app-64.7z\" \"$dir\"",
